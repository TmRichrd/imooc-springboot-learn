<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.mapper.SysUserCustomMapper">
  <select id="getByUserId" resultType="com.imooc.pojo.SysUser" parameterType="string">
    SELECT
        id,`name`
    FROM
        sys_user
    WHERE
        id=#{sid}
  </select>
</mapper>